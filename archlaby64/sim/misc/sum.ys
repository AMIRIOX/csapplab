# must be 0x000 :(
.pos 0x000
main:
    irmovq Stack, %rsp
    irmovq ele1, %rdi
    call sum_list
    halt

# Sample linked list
.align 8
ele1:
    .quad 0x00a
    .quad ele2
ele2:
    .quad 0x0b0
    .quad ele3

ele3:
    .quad 0xc00
    .quad 0

# long sum(list_ptr head)
sum_list:
    xorq %rax, %rax
    andq %rdi, %rdi
    je done

Loop:
    mrmovq (%rdi), %r8
    addq %r8, %rax
    mrmovq 8(%rdi), %rdi
    andq %rdi, %rdi
    jne Loop

done:
    ret

.pos 0x200
Stack:

